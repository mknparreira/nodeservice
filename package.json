      "eslint-plugin-security": "^1.4.0",
      "jest": "^27.5.1",
      "nodemon": "^2.0.15",
      "prettier": "^2.5.1",
      "supertest": "^6.2.2",
      "ts-jest": "^27.1.3",
      "ts-node": "10.4.0",
